     name : bEstRegistre8Bits_PourRM
     procedure_id : 2157233700605533381
     type_code : 12
     code : |1+
      PROCEDURE bEstRegistre8Bits_PourRM()
      
      si  :modeAdres <> MA_Registre ALORS
      	renvoyer faux
      FIN
      selon :registre
      	cas "al","bl","cl","dl"
      		renvoyer Vrai
      	CAS "ah","bh","ch","dh"
      		RENVOYER Vrai		
      	AUTRE CAS
      		renvoyer faux
      FIN
